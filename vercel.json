{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"],
        "env": {
          "DEBUG": "true"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "crons": [
    {
      "path": "/cronjob",
      "schedule": "0 5 * * *"
    }
  ]
}
